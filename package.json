{
  "name": "rspack-plugin-mpa",
  "version": "0.0.1",
  "description": "Mpa plugin for rspack",
  "keywords": [
    "rspack",
    "mpa"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yzydeveloper/rspack-plugin-mpa.git"
  },
