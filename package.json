  "devDependencies": {
    "@rspack/core": "^0.1.9",
    "@rspack/plugin-html": "^0.1.9",
    "@yzydeveloper/eslint-config": "^0.1.5",
    "bumpp": "^8.2.1",
    "eslint": "^8.12.0",
    "tsup": "^6.7.0",
    "typescript": "^4.6.3"
  }
}
